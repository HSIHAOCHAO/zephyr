/ {
	pwmleds: pwmleds {
		compatible = "pwm-leds";
		status = "okay";

		red_pwm_led: red_pwm_led {
			pwms = <&pwm3 1 PWM_MSEC(20) PWM_POLARITY_NORMAL>;
		};
	};

    aliases {
        pwm-led0 = &red_pwm_led;
    };

};

&timers3 {
	st,prescaler = <10000>;
	status = "okay";

	pwm3: pwm {
		status = "okay";
		pinctrl-0 = <&tim3_ch1_pc6>;
		pinctrl-names = "default";
	};
};
